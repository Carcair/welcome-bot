{
    "extends": ["airbnb", "prettier", "plugin:node/recommended", "plugin:security/recommended"],
    "plugins": ["prettier", "security"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "class-methods-use-this": "off"
    }
  }